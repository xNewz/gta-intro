public PayDay()
{
	new string[128];
	new account,interest;
	new rent = 0;
	for(new i = 0; i < MAX_PLAYERS; i++)
	{
		if(IsPlayerConnected(i))
		{
		    if(PlayerInfo[i][pLevel] > 0)
		    {
				new playername2[MAX_PLAYER_NAME];
				new nxtlevel = PlayerInfo[i][pLevel]+1;
				new expamount = nxtlevel*levelexp;
				new infostring[256];
				GetPlayerName(i, playername2, sizeof(playername2));
				account = PlayerInfo[i][pAccount];
				new key = PlayerInfo[i][pPhousekey];
				if(key != 255)
				{
					rent = HouseInfo[key][hRent];
					if(strcmp(playername2, HouseInfo[key][hOwner], true) == 0)
					{
						rent = 0;
					}
					else if(rent > ScriptMoney[i])
					{
						PlayerInfo[i][pPhousekey] = 255;
						SendClientMessage(i, COLOR_WHITE, " คุณไม่มีเงินค่าเช่าบ้านดังนั้นคุณต้องออกจากบ้านเช่า");
						rent = 0;
					}
					HouseInfo[key][hTakings] = HouseInfo[key][hTakings]+rent;
				}
				new tmpintrate;
				if (key != 255 && strcmp(playername2, HouseInfo[key][hOwner], true) == 0)
				{
				    if(PlayerInfo[i][pDonateRank] > 0) { tmpintrate = intrate+4; }
					else { tmpintrate = intrate+2; }//HouseInfo[key][hLevel]
				}
				else
				{
				    if(PlayerInfo[i][pDonateRank] > 0) { tmpintrate = 3; }
					else { tmpintrate = 1; }
				}
				if(PlayerInfo[i][pPayDay] >= 5)
				{
				    Tax += TaxValue;//Should work for every player online
				    PlayerInfo[i][pAccount] -= TaxValue;
				    if(PlayerInfo[i][pDonateRank] > 0)
					{
					    new bonus = PlayerInfo[i][pPayCheck] / 2;
					    PlayerInfo[i][pPayCheck] += bonus;
					}
					new checks = PlayerInfo[i][pPayCheck];
				    new ebill = (PlayerInfo[i][pAccount]/10000)*(PlayerInfo[i][pLevel]);
				    ConsumingMoney[i] = 1;
				    new mayor;
				    new Mtax = MayorInfo[mayor][mTax];
					new Mtaxvalue;
					Mtaxvalue = (checks/100)*(Mtax);
					MayorInfo[mayor][mMoney] += Mtaxvalue;
				    SafeGivePlayerMoney(i, checks);
				    if(PlayerInfo[i][pAccount] > 0)
				    {
				    	PlayerInfo[i][pAccount] -= ebill;
				    	SBizzInfo[4][sbTill] += ebill;
					}
					else
					{
					    ebill = 0;
					}
					if(check3d1[i] == 1)
                    {
                        if(PlayerInfo[i][pDonateRank] < 1)
			            {
					        PlayerInfo[i][pExp] += expx*2;
					        Delete3DTextLabel(test3d1[i]);
					    }

				    	if(PlayerInfo[i][pDonateRank] == 3)
				    	{
                            PlayerInfo[i][pExp] += expx*3;
                            Delete3DTextLabel(test3d1[i]);
				    	}
			        }
			        if(check3d1[i] == 0)
                    {

				    	if(PlayerInfo[i][pDonateRank] >= 2)
				    	{
                            PlayerInfo[i][pExp] += expx*3;
		 				}
    	            }
    	            if(PlayerInfo[i][pDonateRank] == 1)
			        {
			            PlayerInfo[i][pPoint] += 100;
                    }
                    if(PlayerInfo[i][pDonateRank] == 2)
			        {
			            PlayerInfo[i][pPoint] += 300;
                    }
                    if(PlayerInfo[i][pDonateRank] == 3)
			        {
			            PlayerInfo[i][pPoint] += 500;
                    }
                    if(PlayerInfo[i][pDonateRank] == 4)
			        {
			            PlayerInfo[i][pPoint] += 700;
                    }
					interest = (PlayerInfo[i][pAccount]/1000)*(tmpintrate);
					PlayerInfo[i][pExp] += expx;
					PlayerPlayMusic(i);
					SpamPay[i] = 0;
					SpamPun[i] = 0;
					SpamPoy[i] = 0;
					SpamPin[i] = 0;
					SpamFon[i] = 0;
					SpamFin[i] = 0;
					SpamKoy[i] = 0;
					SpamKo[i] = 0;
					SpamKo1[i] = 0;
					PlayerInfo[i][pAccount] = account+interest;
					format(string, sizeof(string), "  เงิน PayDay: $%d   ภาษี: %d เปอร์เซ็น [-$%d]", checks, Mtax, Mtaxvalue);
					SendClientMessage(i, COLOR_GRAD1, string);
					if(PlayerInfo[i][pPhousekey] != 255 || PlayerInfo[i][pPbiskey] != 255)
					{
					    format(string, sizeof(string), "  ค่าไฟฟ้า: -$%d", ebill);
						SendClientMessage(i, COLOR_GRAD1, string);
					}
					format(string, sizeof(string), "  ยอดเงินในบัญชี: $%d", account);
					SendClientMessage(i, COLOR_GRAD1, string);
					format(string, sizeof(string), "  อัตราดอกเบี้ย: 0.%d percent",tmpintrate);
					SendClientMessage(i, COLOR_GRAD2, string);
					format(string, sizeof(string), "  ดอกเบี้ยทีได้รับ $%d", interest);
					SendClientMessage(i, COLOR_GRAD3, string);
					SendClientMessage(i, COLOR_GRAD4, "|============== ธนาาคาร =============|");
					format(string, sizeof(string), "  ยอดเงินในบัญชีใหม่: $%d", PlayerInfo[i][pAccount]);
					SendClientMessage(i, COLOR_GRAD5, string);
					format(string, sizeof(string), "  ค่าเช่า: -$%d", rent);
					SendClientMessage(i, COLOR_GRAD5, string);
					format(string, sizeof(string), "~y~PayDay~n~~w~Paycheck");
					GameTextForPlayer(i, string, 5000, 1);
					rent = 0;
					PlayerInfo[i][pPayDay] = 0;
					PlayerInfo[i][pPayCheck] = 0;
					PlayerInfo[i][pConnectTime] += 1;
					if(check3d1[i] == 0)
					{
                        new BExp = PlayerInfo[i][pBExp];
					    format(string, sizeof(string), "น้ำยา EXPx2: ขณะนี้น้ำยาคูณ 2 ของคุณหมดลงเเล้ว, คุณเหลือขวดน้ำยา: [%d] ขวด",BExp);
					    SendClientMessage(i, COLOR_BLUE, string);
					    Delete3DTextLabel(test3d1[i]);
					    check3d1[i] = 0;
					}
					if(PlayerInfo[i][pExp] >= expamount)
					{
						format(string, sizeof(string), "~g~LEVEL UP~n~~w~You Are Now Level %d", nxtlevel);
						GameTextForPlayer(i, string, 5000, 1);
						PlayerPlaySound(i, 1052, 0.0, 0.0, 0.0);
						PlayerPlayMusic(i);
						PlayerInfo[i][pLevel]++;
						PlayerInfo[i][gPupgrade] = PlayerInfo[i][gPupgrade]+2;
						format(infostring, 256, "* คุณมีค่าอัพเกรดอยู่ %d พอยด์ (พิมพ์ /upgrade เพื่ออัพเกรด)",PlayerInfo[i][gPupgrade]);
						SendClientMessage(i, COLOR_GRAD2, infostring);
						if(PlayerInfo[i][pLevel] > 99)//กันเลเวลเกิน 99
						{
							PlayerInfo[i][pLevel] = 99;
							return 1;
						}
						if(PlayerInfo[i][pDonateRank] > 0)
						{
						    PlayerInfo[i][pExp] -= expamount;
						    new total = PlayerInfo[i][pExp];
						    if(total > 0)
						    {
						        PlayerInfo[i][pExp] = total;
						    }
						    else
						    {
						        PlayerInfo[i][pExp] = 0;
						    }
						}
						else
						{
							PlayerInfo[i][pExp] = 0;
						}
					}
					if(PlayerInfo[i][pGeb] > 0)
					{
					    PlayerInfo[i][pGeb] = 0;
					}
					if(PlayerInfo[i][pDonateRank] > 0)
					{
					    PlayerInfo[i][pPayDayHad] += 1;
					    if(PlayerInfo[i][pPayDayHad] >= 5)
					    {
					        PlayerInfo[i][pExp] += expx;
					        PlayerInfo[i][pPayDayHad] = 0;
					    }
					}
				}
				else
				{
				    SendClientMessage(i, COLOR_LIGHTRED, "* คุณเล่นไม่นานพอที่จะได้ Payday");
				}
			}
		}
	}
	Checkprop();
	return 1;
}
strtok(const string[], &index)
{
	new length = strlen(string);
	while ((index < length) && (string[index] <= ' '))
	{
		index++;
	}

	new offset = index;
	new result[20];
	while ((index < length) && (string[index] > ' ') && ((index - offset) < (sizeof(result) - 1)))
	{
		result[index - offset] = string[index];
		index++;
	}
	result[index - offset] = EOS;
	return result;
}
public split(const strsrc[], strdest[][], delimiter)
{
	new i, li;
	new aNum;
	new len;
	while(i <= strlen(strsrc)){
	    if(strsrc[i]==delimiter || i==strlen(strsrc)){
	        len = strmid(strdest[aNum], strsrc, li, i, 128);
	        strdest[aNum][len] = 0;
	        li = i+1;
	        aNum++;
		}
		i++;
	}
	return 1;
}

