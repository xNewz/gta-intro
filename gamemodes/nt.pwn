public SetPlayerStar(playerid)
{
	if (IsAGangMember(playerid) && gangwar == 1)
	{
	WantedPoints[playerid] = 0;
	WantedLevel[playerid] = 0;
	SetPlayerWantedLevel(playerid, 0);
	SendClientMessage(playerid, COLOR_YELLOW, "Wanted Remove !");
	return 1;
	}
	if(PlayerInfo[playerid][pJailTime] >= 1)
	{
	WantedPoints[playerid] = 0;
	WantedLevel[playerid] = 0;
	SetPlayerWantedLevel(playerid, 0);
	return 1;
	}
	if(ModeDm == 1)
    {
        WantedPoints[playerid] = 0;
        WantedLevel[playerid] = 0;
        SetPlayerWantedLevel(playerid, 0);
        return 1;
    }
	if(eventwanted == 1 || PlayerDeagleStart[playerid] == 1 
	||eventgo[playerid] == 1 || WarKillerG[playerid] == 1)
  	{
  	    WantedPoints[playerid] = 0;
	    WantedLevel[playerid] = 0;
	    SetPlayerWantedLevel(playerid, 0);
	    return 1;
	}
	if(WantedLevel[playerid] == 0)
	{
	    SetPlayerWantedLevel(playerid, 0);
    }
	else if(WantedLevel[playerid] == 1)
	{
	    SetPlayerWantedLevel(playerid, 1);
	    SetTimerEx("Star", 100000, false, "d", playerid);
    }
    else if(WantedLevel[playerid] == 2)
	{
        SetPlayerWantedLevel(playerid, 2);
		SetTimerEx("Star", 100000, false, "d", playerid);
    }
    else if(WantedLevel[playerid] == 3)
	{
        SetPlayerWantedLevel(playerid, 3);
		SetTimerEx("Star", 100000, false, "d", playerid);
    }
    else if(WantedLevel[playerid] == 4)
	{
        SetPlayerWantedLevel(playerid, 4);
		SetTimerEx("Star", 100000, false, "d", playerid);
    }
    else if(WantedLevel[playerid] == 5)
	{
        SetPlayerWantedLevel(playerid, 5);
		SetTimerEx("Star", 100000, false, "d", playerid);
    }
    else if(WantedLevel[playerid] == 6)
	{
        SetPlayerWantedLevel(playerid, 6);
		SetTimerEx("Star", 100000, false, "d", playerid);
    }
	return 1;
