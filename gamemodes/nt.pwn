public OnPlayerTakeDamage(playerid, issuerid, Float: amount, weaponid)
{
	new Float:HP;
	GetPlayerHealth(playerid, HP);
    if(weaponid == 24) SetPlayerHealth(playerid, HP-40);
    if(weaponid == 31) SetPlayerHealth(playerid, HP-9);
    if(weaponid == 29) SetPlayerHealth(playerid, HP-9);
	if(issuerid != INVALID_PLAYER_ID){
		//ตีบวกได้ 10 สองนัดตาย
        if(weaponid == 24 && PlayerInfo[issuerid][pEagle] >= 10 && PlayerInfo[issuerid][pEagle] <= 49){
		    new DMG = PlayerInfo[issuerid][pEagle];
		    if(PlayerInfo[issuerid][pEagle] >= 10 && PlayerInfo[issuerid][pEagle] <= 49) { DMG = 10; }
            SetPlayerHealth(playerid, HP-(25+(5*DMG)));
		}//ตีบวกได้ 50 นัดเดียวตาย
		if(weaponid == 24 && PlayerInfo[issuerid][pEagle] >= 50 && PlayerInfo[issuerid][pEagle] <= 99){
             new DMG = PlayerInfo[issuerid][pEagle];
		     if(PlayerInfo[issuerid][pEagle] >= 100) { DMG = 10; }
             SetPlayerHealth(playerid, HP-(50+(5*DMG)));
		}
		if(weaponid == 24 && PlayerInfo[issuerid][pEagle] >= 100){
		    if(PlayerInfo[issuerid][pEagle] >= 100)
			{
               SetPlayerHealth(playerid, 0);
            }
		}
		if(weaponid == 30 && PlayerInfo[issuerid][pAK47] > 9){
		    new DMG = PlayerInfo[issuerid][pAK47];
		    if(PlayerInfo[issuerid][pAK47] >= 10) { DMG = 10; }
            SetPlayerHealth(playerid, HP-(15+(1*DMG)));
		}
		if(weaponid == 29 && PlayerInfo[issuerid][pMP5] > 9){
		    new DMG = PlayerInfo[issuerid][pMP5];
		    if(PlayerInfo[issuerid][pMP5] >= 10) { DMG = 10; }
            SetPlayerHealth(playerid, HP-(18+(1*DMG)));
		}
    }
    return 1;
}
