public SetPlayerCriminal(playerid,declare,reason[])
{//example: SetPlayerCriminal(playerid,255, "Stealing A Police Vehicle");
    if(StrikeTeam[playerid] != 0)
	{
		return 1;
	}
	if(SniperTeam[playerid] != 0)
	{
		return 1;
	}
	if(IsPlayerConnected(playerid))
	{
	   // PlayerInfo[playerid][pCrimes] += 1;
	    new points = WantedPoints[playerid];
		new turned[MAX_PLAYER_NAME];
		new turner[MAX_PLAYER_NAME];
		new turnmes[128];
		new wantedmes[128];
		new wlevel;
		strmid(PlayerCrime[playerid][pAccusedof], reason, 0, strlen(reason), 255);
		GetPlayerName(playerid, turned, sizeof(turned));
		if (declare == 255)
		{
			format(turner, sizeof(turner), "พลเมืองดี");
			strmid(PlayerCrime[playerid][pVictim], turner, 0, strlen(turner), 255);
		}
		else if (declare == 1)
		{
			format(turner, sizeof(turner), "':::- สำนักงาน ป.ป.ส. -:::'");
			strmid(PlayerCrime[playerid][pVictim], turner, 0, strlen(turner), 1);
		}
		else if (declare == 2)
		{
			format(turner, sizeof(turner), "...:::_หน่วยบัญชาการสงครามพิเศษ_:::...");
			strmid(PlayerCrime[playerid][pVictim], turner, 0, strlen(turner), 2);
		}
		else if (declare == 3)
		{
			format(turner, sizeof(turner), ":: นาวิกโยธิน ::");
			strmid(PlayerCrime[playerid][pVictim], turner, 0, strlen(turner), 3);
		}
		else if (declare == 17)
		{
			format(turner, sizeof(turner), "สหประชาชาติ");
			strmid(PlayerCrime[playerid][pVictim], turner, 0, strlen(turner), 3);
		}
		else
		{
		    if(IsPlayerConnected(declare))
		    {
				GetPlayerName(declare, turner, sizeof(turner));
				strmid(PlayerCrime[playerid][pVictim], turner, 0, strlen(turner), 255);
				strmid(PlayerCrime[declare][pBplayer], turned, 0, strlen(turned), 255);
				strmid(PlayerCrime[declare][pAccusing], reason, 0, strlen(reason), 255);
			}
		}
		format(turnmes, sizeof(turnmes), "* คุณกระทำความผิดข้อหา ( %s ) ผู้รายงาน: %s",reason,turner);
		SendClientMessage(playerid, COLOR_LIGHTRED, turnmes);
		SetPlayerStar(playerid);
		if(points > 0)
		{
		    new yesno;
			if(points >= 1 && points <= 600) { if(WantedLevel[playerid] != 1) { WantedLevel[playerid] = 1; wlevel = 1; yesno = 1; } }
			else if(points >= 601 && points <= 1200)  {if(WantedLevel[playerid] != 2){WantedLevel[playerid] = 2; wlevel = 2; yesno = 1;}}
			else if(points >= 1201 && points <= 1800) { if(WantedLevel[playerid] != 3) { WantedLevel[playerid] = 3; wlevel = 3; yesno = 1; } }
			else if(points >= 1801 && points <= 2400) { if(WantedLevel[playerid] != 4) { WantedLevel[playerid] = 4; wlevel = 4; yesno = 1; } }
			else if(points >= 2401 && points <= 3000) { if(WantedLevel[playerid] != 5) { WantedLevel[playerid] = 5; wlevel = 5; yesno = 1; } }
			else if(points >= 3001 && points <= 3600) { if(WantedLevel[playerid] != 6) { WantedLevel[playerid] = 6; wlevel = 6; yesno = 1; } }
			if(WantedLevel[playerid] >= 1) { if(gTeam[playerid] == 3) { gTeam[playerid] = 4; } }
			if(yesno)
			{
				format(wantedmes, sizeof(wantedmes), " ระดับความต้องการตัว: %d", wlevel);
				SendClientMessage(playerid, COLOR_YELLOW, wantedmes);
				SetPlayerStar(playerid);
				for(new i = 0; i < MAX_PLAYERS; i++)
				{
					if(IsPlayerConnected(i))
					{
					    if(IsACop(i))
					    {
					        format(cbjstore, sizeof(turnmes), "HQ: ทุกหน่วยโปรดทราบ: ผู้รายงาน: %s",turner);
							SendClientMessage(i, TEAM_BLUE_COLOR, cbjstore);
							format(cbjstore, sizeof(turnmes), "HQ: ข้อหา: %s, ผู้กระทำความผิด: %s",reason,turned);
							SendClientMessage(i, TEAM_BLUE_COLOR, cbjstore);
						}
					}
				}
			}
		}
	}
	return 0;
}	//not connected

