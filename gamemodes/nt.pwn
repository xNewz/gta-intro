public Lotto(number)
{
    new JackpotFallen = 0;
    new string[256];
    new winner[MAX_PLAYER_NAME];
    format(string, sizeof(string), "Loterry {FFFFFF}: หวยออกงวดนี้ได้แก่แทนแท๊นแทนนนน หมายเลข : %d.", number);
